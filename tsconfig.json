{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
    "allowJs": true,
		"checkJs": true,
    "types": ["svelte"]
	},
	"include": [
    "src/**/*.d.ts", 
    "src/**/*.ts", 
    "src/**/**/*.d.ts", 
    "src/**/**/*.ts", 
    "src/**/**/*.svelte"
  ],
  "exclude": ["node_modules"]
}
